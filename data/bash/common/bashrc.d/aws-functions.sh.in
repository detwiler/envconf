# AWS functions

function __aws_acct()
{
  if [[ -h @AWS_CONFIG_FILE@ ]]; then
    account="$(basename $(readlink --canonicalize @AWS_CONFIG_FILE@))"
    printf "(aws:$account)"
  fi
} # __aws_acct()


for account in $(command ls @AWS_ACCOUNT_DIR@/* 2>/dev/null); do
  acct=$(basename $account)
  if [[ ! -h $account ]]; then
    eval "function aws-$acct() { @LN_S@ --force $account @AWS_CONFIG_FILE@; }"
    export -f aws-$acct
  fi
done
unset account acct
